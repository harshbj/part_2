<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/backgroundid_fe"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    <android.support.v7.widget.CardView
        android:id="@+id/fesem3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="4dp"
        android:backgroundTint="@color/lightblue"
        android:layout_margin="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DIV A"
            android:textColor="@color/iron"
            android:padding="15dp"
            android:textSize="30dp"
            android:textStyle="bold" />
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/fesem4"
        android:layout_below="@+id/fesem3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="4dp"
        android:backgroundTint="@color/lightblue"
        android:layout_margin="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DIV B"
            android:textColor="@color/iron"
            android:padding="15dp"
            android:textSize="30dp"
            android:textStyle="bold" />
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/fesem5"
        android:layout_below="@+id/fesem4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="4dp"
        android:backgroundTint="@color/lightblue"
        android:layout_margin="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DIV C"
            android:textColor="@color/iron"
            android:padding="15dp"
            android:textSize="30dp"
            android:textStyle="bold" />
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/fesem6"
        android:layout_below="@+id/fesem5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="4dp"
        android:backgroundTint="@color/lightblue"
        android:layout_margin="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DIV D"
            android:textColor="@color/iron"
            android:padding="15dp"
            android:textSize="30dp"
            android:textStyle="bold" />
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/fesem7"
        android:layout_below="@+id/fesem6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="4dp"
        android:backgroundTint="@color/lightblue"
        android:layout_margin="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DIV E"
            android:textColor="@color/iron"
            android:padding="15dp"
            android:textSize="30dp"
            android:textStyle="bold" />
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:id="@+id/fesem8"
        android:layout_below="@+id/fesem7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="4dp"
        android:backgroundTint="@color/lightblue"
        android:layout_margin="5dp"
        android:layout_marginBottom="20dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DIV F"
            android:textColor="@color/iron"
            android:padding="15dp"
            android:textSize="30dp"
            android:textStyle="bold" />
    </android.support.v7.widget.CardView>

        <com.github.chrisbanes.photoview.PhotoView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/photo_fe"
            android:layout_marginBottom="30dp"/>

    </RelativeLayout>

</ScrollView>